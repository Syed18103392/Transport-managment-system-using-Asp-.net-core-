                            </td>
                        </tr>
                        <tr>
                            <td><label asp-for="@Model.Tiretrive.DriverName" class="control-label">Driver Name:</label></td>
                            <td>
                                <input type="text" asp-for="@Model.Tiretrive.DriverName" placeholder="Md.Atikur" class="form-control" />

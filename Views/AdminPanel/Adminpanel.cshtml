<!-- Modal -->
<div class="modal fade" id="ViewTransportList" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document" style="max-width:1000px !important;">
        <div class="modal-content">
            <div class="modal-header">
                <center> <h2 class="modal-title" id="exampleModalLongTitle">Transport List</h2></center>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="facultyRegistrationRequest modal-body uk-panel-scrollable ">
                <table style="border-spacing: 10px">
                    <tr style="padding: 5px">
                        <th style="min-width:100px;">Transport Number</th>
                        <th style="min-width:100px;">Transport Type</th>
                        <th style="min-width:100px;">Driver Name</th>
                        <th style="min-width:100px;">Driver Number</th>
                        <th style="min-width:100px;">Route</th>
                        <th style="min-width:100px;">Status</th>
                    </tr>
                    @foreach (var item in Model.Tishow)
                    {
                        var values = new string[] { item.TransportNumber, item.TransportType, item.DriverName, item.DriverNumber, item.route, item.Status };
                        if (values != null)
                        {
                            <tr style="padding: 5px;border-bottom:1px solid rgba(128, 128, 128, 0.88);">
                                @foreach (var value in values)
                                {
                                    <td style="font-size:14px; min-width:100px;padding:5px 0">

                                        @value

                                    </td>
                                }
                            </tr>
                        }
                        else
                        {
                            <h1>No Transport Available</h1>
                        }
                    }
                </table>
            </div>
            <div class="modal-footer">
                <input type="submit" data-dismiss="modal" value="close" class="btn btn-primary" />
            </div>
        </div>

    </div>
</div>



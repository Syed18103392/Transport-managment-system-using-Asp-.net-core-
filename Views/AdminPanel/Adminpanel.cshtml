<section class="blog_area section_gap">
    <div class="blogcontainer">
        <div class="container " style="max-width: 1312px !important;">
            <div class="row">
                <div class="">
                    <!-- Notification Center -->
                    <center><h2>Notification</h2></center>

                    <hr>
                    <table style="border-spacing: 10px">
                        <tr style="padding: 5px">
                            <th style="min-width:100px;">Sender</th>
                            <th style="min-width:100px;">Purpose</th>
                            <th style="min-width:100px;">Date</th>
                            <th style="min-width:100px;">Time</th>
                            <th style="min-width:100px;">Details</th>
                            <th style="min-width:100px;">Action</th>
                        </tr>
                        <hr>
                        @foreach (var item in Model.Tf)
                        {
                            var values = new string[] { item.Nfrom, item.NFor, item.NComingDate, item.NComingTime, item.NDetails, item.NStatus };
                            if (values != null)
                            {
                                <tr style="padding: 5px;border-bottom:1px solid rgba(128, 128, 128, 0.88);">
                                    @foreach (var value in values)
                                    {
                                        <td style="font-size:14px; min-width:100px;padding:5px 0">
                                            @if (value == "Pending")
                                            {
                                                <div class="NotificationAction">
                                                    <a class="btn btn-success" asp-action="Accept" asp-route-id="@item.id" >Accept</a>
                                                    <a class="btn btn-danger" asp-action="Reject" asp-route-id="@item.id" >Reject</a>
                                                </div>
                                            }
                                            else
                                            {
                                                @value
                                            }
                                        </td>
                                    }
                                </tr>
                            }
                            else
                            {
                                <h1>No Notification</h1>
                            }
                        }
                    </table>
                    <!-- Notificaton Center End -->
                </div>
            </div>
        </div>
    </div>
</section>
